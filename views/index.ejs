<% include header %>
		<script src="/javascripts/jquery-ui.js"></script>
		<div class="zone">
			<form method="post" id="dataForm" action="/safe">
				<ul>
					<li>
						<strong>网页标题</strong><input type="text" name="web" value="" >
					</li>
					<li>
						<strong>上传背景：</strong><input type="file" multiple accept="image/gif,image/png,,image/jpeg" id="files">
						<input type="hidden" name="bg" id="bg" value="" />
					</li>
					<li>
						<strong>网页宽：</strong><input type="text" name="width" value="" class="w01 unity" data-obj="width"/>px
					</li>
					<li>
						<strong>网页高：</strong><input type="text" name="height" value="" class="w01 unity" data-obj="height"/>px
					</li>
					<li id="setBg">
						<strong>背景色：</strong><input type="text" name="bgColor" value="" class="w01 unity" data-obj="bgColor" />
					</li>
					<li>
						<div id="dataArea" class="data_area"></div>
						<input type="submit" name="" id="submit" value="保存" />
					</li>
				</ul>
			</form>
		</div>
		<div class="preview" id="preview">
			<div id="wrap" class="wrap"></div>
			<div id="bgBox"></div>
		</div>

